<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Height="700" Width="700" MinHeight="700" MinWidth="700"
        x:Class="to_do.MainWindow"
        Title="TO-DO App"><!-- Resized="WindowBase_OnResized" -->
    <Window.Styles>
        <StyleInclude Source="src/styles/Main.axaml"></StyleInclude>
    </Window.Styles>
    
    <Grid ShowGridLines="True" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height=".12*"></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height=".25*"></RowDefinition>
        </Grid.RowDefinitions>
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".3*"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        
        <StackPanel Name="Header" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
            <Border Classes="headerStackPanel">
                <TextBlock Classes="headerTextBlock">
                    TO-DO
                </TextBlock>
            </Border>
            
            
        </StackPanel>
        
        <StackPanel Name="SideBar" Grid.Row="1" Grid.Column="0">
        </StackPanel>
        
        <StackPanel Name="Main" Grid.Row="1" Grid.Column="1">
        </StackPanel>
        
        <StackPanel Name="Footer" Grid.Row="2" Grid.ColumnSpan="2" Grid.Column="0">
        </StackPanel>
    </Grid>
</Window>
